{
  "extends": "../../tsconfig.base.json",
  "compilerOptions": {
    "baseUrl": ".",
    "esModuleInterop": true,
    "jsx": "react-jsx",
    "allowJs": true,
    "lib": ["DOM"],
    "moduleResolution": "Node16",
    "noEmit": true,
    "types": [
      "@docusaurus/module-type-aliases",
      "@docusaurus/theme-classic"
    ],
    "paths": {
      "@site/*": ["./*"]
    },
    "skipLibCheck": true
  },
  "include": [
    ".eslintrc.js",
    "src",
    "./*.js"
  ]
}
